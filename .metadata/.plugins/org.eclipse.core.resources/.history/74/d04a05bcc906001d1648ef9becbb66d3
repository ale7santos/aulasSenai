package aulas;

public class PrimeiroExercicio {
	public static void main(String [] args) {
		
		int minhaIdade = 30;
		int idadeProfessor = 32;
		
		System.out.println("Essa é a minha primeira variável: " + minhaIdade);
		System.out.println("Essa é a minha segunda variável: " + idadeProfessor);
		
		
		
	}
}
