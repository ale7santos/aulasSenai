package aulas;

import java.util.Scanner;

public class While {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String usuario;
		String senha;
		
		System.out.println("Entre com o nome do usu치rio");
		usuario = sc.nextLine();
		
		System.out.println("Entre com a senha: ");
		senha = sc.nextLine();
		
		while(usuario.equals(senha)) {
			System.out.println("Usu치rio inv치lido!");
			
			System.out.println("Entre com o nome do usu치rio: ");
			usuario = sc.nextLine();
			
			System.out.println("Entre com a senha: ");
			senha = sc.nextLine();
		}
		
		System.out.println("Cadastro realizado com sucesso!X");
		
		sc.close();

	}

}
