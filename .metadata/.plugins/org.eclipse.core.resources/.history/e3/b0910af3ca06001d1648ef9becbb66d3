package aulas;

public class PrimeiroExercicio {
	public static void main(String [] args) {
		
		int numero1 = 30;
		double numero2 = 30.20;
		
		String nome = "Alessandro";
		char sexo = 'M';
		
		boolean falso = false;
		boolean verdadeiro = true;
		
		System.out.println("Essa é a minha primeira variável: " + numero1);
		System.out.println("Essa é a minha segunda variável: " + numero2);
		System.out.println("Essa é minha variável do tipo String: " + nome);
		System.out.println("Essa é minha variável do tipo char: " + sexo);
		System.out.println("Essa é minha variável do tipo booleana: " + falso);
		System.out.println("Essa é minha variável do tipo booleana com outro valor: " + verdadeiro);
		
		
		
	}
}
